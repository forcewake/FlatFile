    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug - Benchmarks",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/FlatFile.Benchmark/bin/Debug/netcoreapp2.1/FlatFile.Benchmarks.dll",
            "args": [],
            "cwd": "${workspaceFolder}/src",
            "console": "internalConsole",
            "stopAtEntry": false,
            "internalConsoleOptions": "openOnSessionStart",
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Local"
            }
        },
        {
            "name": "Run Benchmarks",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "releaseBuild",
            "program": "${workspaceFolder}/FlatFile.Benchmark/bin/Release/netcoreapp2.1/FlatFile.Benchmarks.dll",
            "args": [],
            "cwd": "${workspaceFolder}/src",
            "console": "internalConsole",
            "stopAtEntry": false,
            "internalConsoleOptions": "openOnSessionStart",
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Local"
            }
        }
    ]
}